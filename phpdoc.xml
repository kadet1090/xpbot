<?xml version="1.0" encoding="utf-8"?>
<phpdoc>
    <parser>
        <target>Documentation</target>
    </parser>
    <transformer>
        <target>Documentation</target>
        <template name="clean"/>
    </transformer>
    <files>
        <directory>.</directory>
        <ignore>Plugins/*</ignore>
        <ignore>Documentation/*</ignore>
        <ignore>Tests/*</ignore>
        <ignore>Logs/*</ignore>
        <ignore>Config/*</ignore>
    </files>
</phpdoc>